//---------------------------------------------------------------------------

#pragma hdrstop

#include "ClassUnit.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)


void Ill::AddSymptom(string symptom)
{
	symptoms.assign(1,symptom);

}
void Ill::AddOrgan(string organ)
{
	organs.assign(1,organ);

}
list<string> Ill::GetSymptoms()
{
	return symptoms;
}
list<string> Ill::GetOrgans()
{
	 return organs;
}
string Ill::GetName()
{
	return name;
}
string Ill::GetDesc()
{
	return desc;
}

